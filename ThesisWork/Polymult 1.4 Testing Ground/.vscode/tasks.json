{
    "tasks": [
        {
            "type": "cppbuild",
            "label": "C LOCAL DEBUG builder Linux",
            "command": "/usr/bin/gcc",
            "args": [
                "-o",
                "polymult",
                "-Wall",
                "-O3",
                "-g",
                "-Wextra",
                "-Wconversion",
                "-Wsign-conversion",
                "-fopenmp",
                "main.c",
                "mult.c",
                "init.c",
                "-lm",
                "-lflint",
                "-lgmp",
            ],
            "options": {
                "cwd": "${fileDirname}"
            },
            "problemMatcher": [
                "$gcc"
            ],
            "group": {
                "kind": "build",
                "isDefault": true
            },
            "detail": "Task generated by Debugger."
        }
    ],
    "version": "2.0.0"
}