{
    "tasks": [
        {
            "type": "cppbuild",
            "label": "C DEBUG builder Linux",
            "command": "/usr/bin/gcc",
            "args": [
                "-g",
                "-Wall",
                "-Weffc++",
                "-Wextra",
                "-Wconversion",
                "-Wsign-conversion",
                "-std=c++20",
                "${file}",
                "-o",
                "${fileBasenameNoExtension}.exe"
            ],
            "options": {
                "cwd": "${fileDirname}"
            },
            "problemMatcher": [
                "$gcc"
            ],
            "group": {
                "kind": "build",
                "isDefault": true
            },
            "detail": "Task generated by Debugger."
        }
    ],
    "version": "2.0.0"
}